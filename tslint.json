{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": [true, "never"],
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "time", "timeEnd", "trace"]
    }
  },
  "rulesDirectory": []
}
